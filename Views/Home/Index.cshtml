@model IEnumerable<dynamic>

<h2>Thống Kê Nhân Viên Theo Phòng Ban</h2>

<table class="table">
    <thead>
        <tr>
            <th>Phòng Ban</th>
            <th>Số Lượng Nhân Viên</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.DepartmentName</td>
                <td>@item.EmployeeCount</td>
            </tr>
        }
    </tbody>
</table>

